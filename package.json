{
  "name": "mob-timer",
  "version": "0.1.0",
  "dependencies": {
    "material-ui": "^0.20.0",
    "normalize.css": "^8.0.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.14.0",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "rxjs": "^5.5.8",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/material-ui": "^0.21.1",
    "@types/node": "^9.6.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "husky": "^0.14.3",
    "prettier": "^1.11.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
